actor intro_fire : switchabledecoration  22213 {
//$sprite INTRA0
//$Category INTROS
	+NOINTERACTION
	+NOGRAVITY
	+WALLSPRITE
	renderstyle "add"
	alpha 0.0

	States
	{
		Spawn:
			TNT1 A 1
			GOTO Inactive
		Inactive:
			TNT1 A 1
			Loop
		Active:
			INTR AAAA 1 A_FadeIn(0.25)
			INTR A 1
			Loop

	}

}

actor intro_caleb_0 : switchabledecoration  22214 {
//$sprite ICLBA0
//$Category INTROS
	+NOINTERACTION
	+NOGRAVITY
	+WALLSPRITE
	alpha 1.0
	RenderStyle "Translucent"

	States
	{
		Spawn:
			TNT1 A 1
